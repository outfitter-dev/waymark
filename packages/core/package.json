{
  "name": "@waymarks/core",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build:types && bun build src/index.ts --outdir dist --target bun",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "dev": "bun build src/index.ts --outdir dist --target bun --watch",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "lint": "bunx ultracite check",
    "check:waymarks": "echo 'Waymarks check will be implemented in v1.0'"
  },
  "dependencies": {
    "@waymarks/grammar": "workspace:*",
    "pino": "^9.11.0",
    "type-fest": "^5.0.1"
  },
  "devDependencies": {
    "@types/bun": "1.2.22"
  }
}
